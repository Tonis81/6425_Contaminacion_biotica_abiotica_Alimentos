	keywords = {Age,Arsenic,Developmental immunotoxicity,Immune system,Immunotoxicity,Sex},
	number = {1},
	pages = {1--12},
	pmid = {26875182},
	title = {Arsenic Exposure and Immunotoxicity: a Review Including the Possible Influence of Age and Sex},
