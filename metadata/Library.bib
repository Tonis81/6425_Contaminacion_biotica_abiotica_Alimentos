	doi = {10.1007/s12403-020-00371-8},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Signes-Pastor et al. - 2020 - Exposure to a Mixture of Metals and Growth Indicators in 6--11-Year-Old Children from the 2013--2016 NHA.pdf:pdf},
	isbn = {0123456789},
	issn = {24519685},
	journal = {Exposure and Health},
	keywords = {Body size,Childhood exposure,Growth,Metal mixture,NHANES},
	number = {0123456789},
	publisher = {Springer Netherlands},
	title = {{Exposure to a Mixture of Metals and Growth Indicators in 6--11-Year-Old Children from the 2013--2016 NHANES}},
	url = {https://doi.org/10.1007/s12403-020-00371-8},
