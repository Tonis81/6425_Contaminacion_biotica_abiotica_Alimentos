	journal = {Current Environmental Health Reports},
	keywords = {adult,alzheimer,arsenate,arsenic,arsenite,children,cholinergic,cognition,development,exposure,glutamatergic,hpa,monoaminergic,neurogenesis,neurotoxicity,s disease,teratogen},
	language = {eng},
